   "source": [
    "num = int(input(\"Please input a number: \"))\n",
    " \n",
    "if num > 1:\n",
    "   for i in range(2,num):\n",
    "       if (num % i) == 0:\n",
    "           print(num,\"不是質數\")\n",
    "           print(\"因為\",i,\"乘\",num//i,\"是\",num)\n",
    "           break\n",
    "   else:\n",
    "       print(num,\"是質數\")\n",
    "       \n",
    "else:\n",
    "   print(num,\"不是質數\")"
